{
    "name": "amir-anisheh/scrapeflow",
    "description": "A chainable, SOAP-like HTML parser for Laravel using pure PHP",
    "type": "library",
    "license": "MIT",
    "version": "1.2",

    "authors": [
        {
            "name": "Amir Mohamad Anisheh",
            "email": "amirtaja@yahoo.com"
        }
    ],
   "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0"
  },
    "autoload": {
        "psr-4": {
            "AmirAnisheh\\ScrapeFlow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmirAnisheh\\ScrapeFlow\\ScrapeFlowProvider"
            ],
            "aliases": {
                "ScrapeFlow": "AmirAnisheh\\ScrapeFlow\\Facades\\ScrapeFlow"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
